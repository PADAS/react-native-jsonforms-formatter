{
  "name": "react-native-jsonforms-formatter",
  "version": "1.0.0",
  "description": "Converts JTD into JSON Schema ",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "lint": "xo --fix",
    "test": "jest src/.*\\.test\\..* --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PADAS/react-native-jsonforms-formatter.git"
  },
  "keywords": [
    "json",
    "schema",
    "forms",
    "JTD",
    "UI",
    "formatter",
    "validator",
    "validation"
  ],
  "author": "Earth Ranger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PADAS/react-native-jsonforms-formatter/issues"
  },
  "homepage": "https://github.com/PADAS/react-native-jsonforms-formatter#readme",
  "dependencies": {
    "@jsonforms/core": "^3.0.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/lodash-es": "^4.17.7",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./extJestSetup.js"
    ],
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    },
    "collectCoverageFrom": [
      "!<rootDir>/src/common/mockData/*.{ts,tsx}",
      "!**/*.png"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
